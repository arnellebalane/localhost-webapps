{
  "manifest_version": 2,
  "name": "Localhost WebApps",
  "description": "Links to web applications in localhost",
  "version": "0.1",
  "background": {
    "scripts": ["javascripts/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["http://*/", "storage"]
}